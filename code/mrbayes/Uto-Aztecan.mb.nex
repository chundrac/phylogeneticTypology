#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Uto-Aztecan.nex;
		lset rates=gamma coding=all;
		constraint c1 = UA.AZTECAN.TETELCINGO_NAHUATL UA.AZTECAN.NAHUATL_POMARO_AQUILA UA.AZTECAN.PIPIL UA.CORACHOL.EL_NAYAR_CORA UA.CAHITA.EUDEVE UA.CAHITA.OPATA UA.CAHITA.YAQUI UA.CAHITA.YAQUI_2 UA.TEPIMAN.LOWER_PIMA UA.TEPIMAN.PIMA_BAJO UA.TEPIMAN.TOHONO_OODHAM UA.TEPIMAN.UPPER_PIMA UA.TEPIMAN.NORTHERN_TEPEHUAN UA.TARAHUMARAN.CENTRAL_TARAHUMARA;
		constraint c2 = UA.HOPI.HOPI UA.NUMIC.SOUTHERN_PAIUTE UA.NUMIC.UTE_1 UA.NUMIC.UTE_2 UA.NUMIC.UTE_SOUTHERN_PAIUTE UA.NUMIC.NORTHERN_PAIUTE UA.NUMIC.NORTHERN_PAIUTE_BANNOCK UA.NUMIC.COMANCHE UA.CALIFORNIA_UTO_AZTECAN.CAHUILLA UA.CALIFORNIA_UTO_AZTECAN.CUPENO UA.CALIFORNIA_UTO_AZTECAN.LUISENO;
		constraint c3 = UA.AZTECAN.TETELCINGO_NAHUATL UA.AZTECAN.NAHUATL_POMARO_AQUILA UA.AZTECAN.PIPIL UA.CORACHOL.EL_NAYAR_CORA;
		constraint c4 = UA.CAHITA.EUDEVE UA.CAHITA.OPATA;
		constraint c5 = UA.CAHITA.YAQUI UA.CAHITA.YAQUI_2;
		constraint c6 = UA.TEPIMAN.LOWER_PIMA UA.TEPIMAN.PIMA_BAJO UA.TEPIMAN.TOHONO_OODHAM UA.TEPIMAN.UPPER_PIMA UA.TEPIMAN.NORTHERN_TEPEHUAN;
		constraint c7 = UA.NUMIC.SOUTHERN_PAIUTE UA.NUMIC.UTE_1 UA.NUMIC.UTE_2 UA.NUMIC.UTE_SOUTHERN_PAIUTE UA.NUMIC.NORTHERN_PAIUTE UA.NUMIC.NORTHERN_PAIUTE_BANNOCK UA.NUMIC.COMANCHE;
		constraint c8 = UA.CALIFORNIA_UTO_AZTECAN.CAHUILLA UA.CALIFORNIA_UTO_AZTECAN.CUPENO UA.CALIFORNIA_UTO_AZTECAN.LUISENO;
		constraint c9 = UA.AZTECAN.TETELCINGO_NAHUATL UA.AZTECAN.NAHUATL_POMARO_AQUILA UA.AZTECAN.PIPIL;
		constraint c10 = UA.TEPIMAN.LOWER_PIMA UA.TEPIMAN.PIMA_BAJO UA.TEPIMAN.TOHONO_OODHAM UA.TEPIMAN.UPPER_PIMA;
		constraint c11 = UA.NUMIC.SOUTHERN_PAIUTE UA.NUMIC.UTE_1 UA.NUMIC.UTE_2 UA.NUMIC.UTE_SOUTHERN_PAIUTE;
		constraint c12 = UA.NUMIC.NORTHERN_PAIUTE UA.NUMIC.NORTHERN_PAIUTE_BANNOCK;
		constraint c13 = UA.CALIFORNIA_UTO_AZTECAN.CAHUILLA UA.CALIFORNIA_UTO_AZTECAN.CUPENO;
		constraint c14 = UA.AZTECAN.TETELCINGO_NAHUATL UA.AZTECAN.NAHUATL_POMARO_AQUILA;
		prset topologypr = constraints(c1,c2,c3,c4,c5,c6,c7,c8,c9,c10,c11,c12,c13,c14);
		prset brlenspr = clock:uniform;
		prset clockvarpr = igr;
		prset treeagepr=Gamma(0.05, 0.005);
		prset shapepr=Exponential(10);
		set beagleprecision=double;
		mcmcp Burninfrac=0.5 stoprule=no stopval=0.01;
		mcmcp filename=../data/asjpNex/output/Uto-Aztecan;
		mcmcp samplefreq=1000 printfreq=5000 append=no;
		mcmc ngen=1000000 nchains=4 nruns=2;
		sump;
		sumt;
	end;
