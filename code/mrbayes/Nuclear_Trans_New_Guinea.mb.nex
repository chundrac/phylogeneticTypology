#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Nuclear_Trans_New_Guinea.nex;
		lset rates=gamma coding=all;
		constraint c1 = TNG.EASTERN_HIGHLANDS.AWA TNG.EASTERN_HIGHLANDS.AWA_2 TNG.EASTERN_HIGHLANDS.YAGARIA TNG.EASTERN_HIGHLANDS.FORE;
		constraint c2 = TNG.FINISTERRE_HUON.NANKINA TNG.FINISTERRE_HUON.NABAK;
		constraint c3 = TNG.MADANG.AMELE TNG.MADANG.WANUMA TNG.MADANG.KOBON TNG.MADANG.KALAM TNG.MADANG.IKUNDUN TNG.MADANG.SUROI TNG.MADANG.TAUYA TNG.MADANG.WASKIA;
		constraint c4 = TNG.MEK.EIPOMEK TNG.MEK.UNA TNG.MEK.YALE_KOSAREK;
		constraint c5 = TNG.OK.TELEFOL TNG.OK.MIAN TNG.AWJU_DUMUT.WAMBON;
		constraint c6 = TNG.BINANDEREAN.BINANDERE TNG.BINANDEREAN.MAMBARE_RIVER TNG.BINANDEREAN.SUENA;
		constraint c7 = TNG.CHIMBU.KUMAN TNG.CHIMBU.GOLIN;
		constraint c8 = TNG.DANI.HITIGIMA_DANI TNG.DANI.TANGMA_DANI TNG.DANI.UPPER_PYRAMID_DANI;
		constraint c9 = TNG.EASTERN_HIGHLANDS.AWA TNG.EASTERN_HIGHLANDS.AWA_2;
		constraint c10 = TNG.EASTERN_HIGHLANDS.YAGARIA TNG.EASTERN_HIGHLANDS.FORE;
		constraint c11 = TNG.MADANG.AMELE TNG.MADANG.WANUMA;
		constraint c12 = TNG.MADANG.KOBON TNG.MADANG.KALAM TNG.MADANG.IKUNDUN;
		constraint c13 = TNG.MADANG.SUROI TNG.MADANG.TAUYA;
		constraint c14 = TNG.MEK.EIPOMEK TNG.MEK.UNA;
		constraint c15 = TNG.OK.TELEFOL TNG.OK.MIAN;
		constraint c16 = TNG.BINANDEREAN.BINANDERE TNG.BINANDEREAN.MAMBARE_RIVER;
		constraint c17 = TNG.MADANG.KOBON TNG.MADANG.KALAM;
