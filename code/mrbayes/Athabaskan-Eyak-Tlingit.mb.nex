#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Athabaskan-Eyak-Tlingit.nex;
		lset rates=gamma coding=all;
		constraint c1 = NDe.ATHAPASKAN.CHIPEWYAN NDe.ATHAPASKAN.KOYUKON NDe.ATHAPASKAN.SARCEE NDe.ATHAPASKAN.TANACROSS NDe.ATHAPASKAN.NAVAHO NDe.ATHAPASKAN.NAVAJO NDe.ATHAPASKAN.HUPA NDe.ATHAPASKAN.HUPA_2;
		constraint c2 = NDe.ATHAPASKAN.CHIPEWYAN NDe.ATHAPASKAN.KOYUKON NDe.ATHAPASKAN.SARCEE NDe.ATHAPASKAN.TANACROSS;
		constraint c3 = NDe.ATHAPASKAN.NAVAHO NDe.ATHAPASKAN.NAVAJO;
		constraint c4 = NDe.ATHAPASKAN.HUPA NDe.ATHAPASKAN.HUPA_2;
		prset topologypr = constraints(c1,c2,c3,c4);
		prset brlenspr = clock:uniform;
		prset clockvarpr = igr;
		prset treeagepr=Gamma(0.05, 0.005);
		prset shapepr=Exponential(10);
		set beagleprecision=double;
		mcmcp Burninfrac=0.5 stoprule=no stopval=0.01;
		mcmcp filename=../data/asjpNex/output/Athabaskan-Eyak-Tlingit;
		mcmcp samplefreq=1000 printfreq=5000 append=no;
		mcmc ngen=1000000 nchains=4 nruns=2;
		sump;
		sumt;
	end;
