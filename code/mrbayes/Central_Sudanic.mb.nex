		constraint c5 = CSu.BONGO_BAGIRMI.BAKA_SUDAN CSu.BONGO_BAGIRMI.MODO;
		constraint c6 = CSu.MORU_MAADI.LOGO CSu.MORU_MAADI.LUGBARA;
		constraint c7 = CSu.MORU_MAADI.LOKAI CSu.MORU_MAADI.MAADI CSu.MORU_MAADI.PANDIKERI;
