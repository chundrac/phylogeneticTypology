#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Turkic.nex;
		lset rates=gamma coding=all;
		constraint c1 = Alt.TURKIC.BASHKIR Alt.TURKIC.KAZAN_TATAR Alt.TURKIC.MISHER_TATAR Alt.TURKIC.TATAR Alt.TURKIC.KARACHAY_BALKAR Alt.TURKIC.KARAKALPAK Alt.TURKIC.KHALAJ_IRAN Alt.TURKIC.TURKMEN Alt.TURKIC.TURKMEN_2 Alt.TURKIC.TURKISH Alt.TURKIC.TURKISH_2 Alt.TURKIC.SAKHA Alt.TURKIC.TUVAN;
		constraint c2 = Alt.TURKIC.BASHKIR Alt.TURKIC.KAZAN_TATAR Alt.TURKIC.MISHER_TATAR Alt.TURKIC.TATAR Alt.TURKIC.KARACHAY_BALKAR Alt.TURKIC.KARAKALPAK;
		constraint c3 = Alt.TURKIC.TURKMEN Alt.TURKIC.TURKMEN_2 Alt.TURKIC.TURKISH Alt.TURKIC.TURKISH_2;
		constraint c4 = Alt.TURKIC.BASHKIR Alt.TURKIC.KAZAN_TATAR Alt.TURKIC.MISHER_TATAR Alt.TURKIC.TATAR;
		constraint c5 = Alt.TURKIC.TURKMEN Alt.TURKIC.TURKMEN_2;
