#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Pano-Tacanan.nex;
		lset rates=gamma coding=all;
		constraint c1 = Pan.PANOAN.CASHIBO Pan.PANOAN.KASHIBO_ALTO_AGUAYTIA Pan.PANOAN.KASHIBO_BAJO_AGUAYTIA Pan.PANOAN.KASHIBO_SAN_ALEJANDRO Pan.PANOAN.KASHIBO_SUNGAROYACU Pan.PANOAN.KONIBO Pan.PANOAN.SHIPIBO;
		constraint c2 = Pan.PANOAN.KONIBO Pan.PANOAN.SHIPIBO;
		prset topologypr = constraints(c1,c2);
		prset brlenspr = clock:uniform;
		prset clockvarpr = igr;
		prset treeagepr=Gamma(0.05, 0.005);
		prset shapepr=Exponential(10);
		set beagleprecision=double;
		mcmcp Burninfrac=0.5 stoprule=no stopval=0.01;
		mcmcp filename=../data/asjpNex/output/Pano-Tacanan;
		mcmcp samplefreq=1000 printfreq=5000 append=no;
		mcmc ngen=1000000 nchains=4 nruns=2;
		sump;
		sumt;
	end;
