#Nexus
	Begin MrBayes;
		set seed=6789580436154794230;
		set swapseed = 614090213;
		execute ../data/asjpNex/Pama-Nyungan.nex;
		lset rates=gamma coding=all;
		constraint c1 = PN.NORTHERN_PAMA_NYUNGAN.WIK_MUNGKAN PN.NORTHERN_PAMA_NYUNGAN.KUKU_UWANH PN.NORTHERN_PAMA_NYUNGAN.THAYORE;
		constraint c2 = PN.CENTRAL_PAMA_NYUNGAN.AKARA PN.CENTRAL_PAMA_NYUNGAN.ALYAWARR_ALYAWARA PN.CENTRAL_PAMA_NYUNGAN.ALYAWARR_MACDONALDDOWNS_ALYAWARA PN.CENTRAL_PAMA_NYUNGAN.ALYAWARRA PN.CENTRAL_PAMA_NYUNGAN.ARANDA PN.CENTRAL_PAMA_NYUNGAN.ARRARNTA_WESTERN PN.CENTRAL_PAMA_NYUNGAN.ARRERNTE_CENTRAL PN.CENTRAL_PAMA_NYUNGAN.ARRERNTE_EASTERN PN.CENTRAL_PAMA_NYUNGAN.ARRERNTE_EASTERN_2 PN.CENTRAL_PAMA_NYUNGAN.LOWER_ARANDA PN.CENTRAL_PAMA_NYUNGAN.SOUTHERN_ARANDA PN.CENTRAL_PAMA_NYUNGAN.WESTERN_ARRERNTE;
		constraint c3 = PN.CENTRAL_PAMA_NYUNGAN.YANDRUWANDHA PN.CENTRAL_PAMA_NYUNGAN.DIYARI;
		constraint c4 = PN.WESTERN_PAMA_NYUNGAN.NGAANYATJARRA PN.WESTERN_PAMA_NYUNGAN.WANGKA_WIRU;
		constraint c5 = PN.NORTHERN_PAMA_NYUNGAN.BIRI PN.NORTHERN_PAMA_NYUNGAN.BIRRI_AUSTRALIA PN.NORTHERN_PAMA_NYUNGAN.WIRRI;
		constraint c6 = PN.NORTHERN_PAMA_NYUNGAN.WIK_MUNGKAN PN.NORTHERN_PAMA_NYUNGAN.KUKU_UWANH;
		prset topologypr = constraints(c1,c2,c3,c4,c5,c6);
		prset brlenspr = clock:uniform;
		prset clockvarpr = igr;
		prset treeagepr=Gamma(0.05, 0.005);
		prset shapepr=Exponential(10);
		set beagleprecision=double;
		mcmcp Burninfrac=0.5 stoprule=no stopval=0.01;
		mcmcp filename=../data/asjpNex/output/Pama-Nyungan;
		mcmcp samplefreq=1000 printfreq=5000 append=no;
		mcmc ngen=1000000 nchains=4 nruns=2;
		sump;
		sumt;
	end;
